{
  "name": "tvnav",
  "version": "1.0.0",
  "description": "Navega canales de television y radio en vivo.",
  "main": "index.html",
  "scripts": {
    "test2": "node ./rmDir.js && webpack --env=prod --profile",
    "dev": "webpack-dev-server --host localhost --port 8080 --env=dev --inline --hot --profile --open",
    "hotDebug": "webpack-dev-server --host localhost --port 8080 --env=dev --inline --hot --profile",
    "debug": "webpack-dev-server --host localhost --port 8080 --env=dev --profile",
    "net2": "webpack-dev-server --host 192.168.0.130 --port 80 --env=dev --profile --open",
    "watch": "webpack --progress --colors --env=dev --profile --watch",
    "exportp": "node ./rmDir.js && webpack --env=prod --profile --progress",
    "rm": "node ./rmDir.js",
    "clean": "node ./clean.js && npm i -D && npm up -D",
    "favicon": "real-favicon generate faviconDescription.json faviconData.json ./dist/",
    "faviconInject": "real-favicon inject faviconData.json ./src/ ./src/index.ejs",
    "faviconUpdate": "real-favicon check-for-update --fail-on-update faviconData.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtreemze/TVNav.git"
  },
  "keywords": [
    "progressive",
    "webapp",
    "JavaScript"
  ],
  "author": "Carlos Velasco",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/xtreemze/TVNav/issues"
  },
  "homepage": "https://github.com/xtreemze/TVNav#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
