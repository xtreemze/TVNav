{
  "name": "tvnav",
  "version": "1.0.0",
  "description": "Navega canales de television y radio en vivo.",
  "main": "index.html",
  "scripts": {
    "test": "none",
    "dev": "webpack-dev-server --host localhost --env=dev --inline --hot --profile --open",
    "debugHot": "webpack-dev-server --host localhost --port 8080 --env=dev --inline --hot --profile",
    "debug": "webpack-dev-server --host localhost --port 8080 --env=dev --profile",
    "net": "webpack-dev-server --public --host 192.168.0.170 --port 80 --env=dev --profile --open",
    "net2": "webpack-dev-server --public --host 192.168.0.117 --port 80 --env=dev --profile --open",
    "devHTML": "webpack-dev-server --progress --colors --host localhost --port 8080 --env=dev --profile --open",
    "watch": "webpack --progress --colors --env=dev --profile --watch",
    "exportp": "webpack --env=prod --profile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtreemze/TVNav.git"
  },
  "keywords": [
    "progressive",
    "webapp",
    "JavaScript"
  ],
  "author": "Carlos Velasco",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/xtreemze/TVNav/issues"
  },
  "homepage": "https://github.com/xtreemze/TVNav#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.6.1",
    "browserslist": "^2.7.0",
    "css-loader": "^0.26.4",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.10.1",
    "fscreen": "^1.0.2",
    "google-closure-compiler-js": "^20170910.0.1",
    "html-loader": "^0.4.5",
    "html-minifier-webpack-plugin": "^1.1.1",
    "image-webpack-loader": "^3.4.2",
    "offline-plugin": "^4.8.4",
    "smoothscroll-polyfill": "^0.4.0",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "devDependencies": {
    "postcss-loader": "^2.0.8",
    "video.js": "^6.4.0",
    "videojs-contrib-hls": "^5.12.1",
    "videojs-contrib-media-sources": "^4.6.1",
    "videojs-flash": "^2.0.1",
    "webworkify": "^1.4.0",
    "webworkify-webpack-dropin": "^1.1.9",
    "zenscroll": "^4.0.0"
  }
